const a = 5;
const b = 2;
const myName = "joy";
//길이가 긴 변수를 선언할 때, 다음 단어는 대문자로 시작한다. ex) myName, veryLongVariableName (=낙타 등과 닮았다고 camelCase)
//Python 에서는 very_long_variable_name으로 표기한다.(=뱀과 닮았다고 snake_case) + const를 따로 표기하지 않아도 된다. ex) my_name = "joy"

console.log(a+b);
console.log(a*b);
console.log(a/b);
console.log("hello "+ myName);
